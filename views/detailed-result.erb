<section class="analysis-result detailed-result">
  <ul>
    <% @separated_results.each_pair do |phrase, data| %>
      <%= "<li class=\"#{data[2]}\"
              title=\"#{data[0]} positive, #{data[1]} negative\">#{phrase}</li>" %>
    <% end %>
  </ul>
  <span class="result">

  </span>

  <form method="get" action="<%= url '/' %>">
    <button type="submit" class="goback">Back</button>
  </form>

</section>
